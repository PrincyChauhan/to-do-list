<%- include("header") %> 
    <div class="box" id="heading">
        <h1> <%=listTitle%></h1>

    </div>

    <div class="box">
        <%newItemList.forEach((item)=>{%>

            <div class="item">
                <input type="checkbox">
                <p><%=item.name%></p>
            </div>
        <%})%>
                <form action="/" class="item" method="POST">
                <input type="text" name="newItem" placeholder="New Item" required>
                <button type="submit" name="list" value=<%=listTitle%>>+</button>
            </form>
    </div>
 
    <%- include("footer")%> 